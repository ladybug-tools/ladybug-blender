{
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "ladybug-blender.zip",
                        "name": "ladybug-blender-${nextRelease.version}.zip",
                        "label": "ladybug-blender-${nextRelease.version}.zip"
                    }
                ]
            }
        ]
    ]
}
